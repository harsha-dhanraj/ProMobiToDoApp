<% if @message.nil? %>
	bootbox.alert({size: "small",title: "<i class='fa fa-check'></i> Success!",message: "Project created successfully!"}).find('.modal-header').css({"background-color":"#1AAF5D"});
	$('.project-list').html("<%= j render 'index' %>");
	$('.project-list #developers').chosen({
		width: '100%'
	});
	$('.project-list #project_name').val('')
	$('.project-list #project_description').val('')
	$('.project-list #project_client').val('')
<% else %>
	bootbox.alert({size: "small",title: "<i class='fa fa-exclamation-triangle'></i> Please Review",message: "<%= @message %>"});
<% end %>